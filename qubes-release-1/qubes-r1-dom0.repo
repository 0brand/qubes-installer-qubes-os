[qubes]
name = Qubes OS Repository
baseurl = http://qubes-os.org/yum/r1/dom0/rpm
enabled = 1
metadata_expire = 7d
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-qubes-x86_64

[qubes-testing]
name = Qubes OS Repository
baseurl = http://qubes-os.org/yum/r1-testing/dom0/rpm
enabled = 1
metadata_expire = 7d
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-qubes-x86_64

